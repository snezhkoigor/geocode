{
    "name": "snezhkoigor/geocoding",
    "description": "A development package for all providers",
    "keywords": ["geocode", "laravel"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Snezhko I.S.",
            "email": "i.s.sergeevich@yandex.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "5.0 - 5.8",
        "illuminate/database": "5.8.*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
